{
  "name": "code-batch",
  "version": "1.0.0",
  "description": "Module for converting characters to code points, binary, hex. It fixes the javascript charCodeAt to use in loops. Can convert emojis.",
  "type": "module",
  "author": "Cal Young",
  "keywords": [
    "charCodeAt",
    "code",
    "code-points",
    "hexidecimal",
    "decimal",
    "char",
    "binary",
    "utf-32",
    "utf-16",
    "utf-8",
    "encode",
    "decode",
    "charcode",
    "nodejs",
    "codepoint",
    "string",
    "javascript",
    "module",
    "node"
  ],
  "main": "./index.cjs",
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs"
  },
  "types": "./types/index.d.ts",
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "standard": "^16.0.4"
  },
  "dependencies": {
    "@neumatter/is": "^1.0.2"
  },
  "babel": {
    "targets": {
      "node": "16"
    }
  },
  "standard": {
    "parser": "@babel/eslint-parser",
    "ignore": [
      "/types"
    ]
  }
}
